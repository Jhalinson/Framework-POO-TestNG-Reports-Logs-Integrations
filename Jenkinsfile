pipeline{
  agent any
   def mvnHome
    stages{
      stage("clean"){
        mvnHome = tool 'Maven'
        
        echo $mvnHome
        steps{
            echo 'Run clean tests here...'
    }
    stages{
      stage("build"){
        steps{
          echo 'Run BUILD tests here...'
    }
    stages{
      stage("test"){
        steps{
          
          echo 'Run TEST OR INSTALL tests here...'
    }
  
  }
}
}


